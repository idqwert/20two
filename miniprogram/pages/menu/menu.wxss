.menu-nav{
  width: 100%;
  height: 80rpx;
  background-color: white;
  position: fixed;
  z-index: 2;
}
.person{
  position: absolute;
  width: 60rpx;
  height: 60rpx;
  top:10rpx;
  left:20rpx;
}
.person image{
  width: 100%;
  height: 100%;
}
.nav-item:nth-child(2){
  position: absolute;
  width: 120rpx;
  height: 76rpx;
  text-align: center;
  line-height: 76rpx;
  left:225rpx;
}
.nav-item:nth-child(3){
  position: absolute;
  width: 120rpx;
  height: 76rpx;
  text-align: center;
  line-height: 76rpx;
  left:400rpx;
}
.active{
  color:#c16d78;
  border-bottom: 4rpx solid #c16d78;
}

.search{
  position: absolute;
  width: 60rpx;
  height: 60rpx;
  top:10rpx;
  right:20rpx;
}
.search image{
  width: 100%;
  height: 100%;
}
.swiper{
  width: 100%;
  height: 280rpx;
  margin-top: 80rpx;
}
.swiper image{
  width: 100%;
  height: 100%;
}
.tui{
  font-size: 40rpx;
  font-weight: bolder;
  margin-top: 40rpx;
  text-align: center;
  color:#c16d78;
}

.variety-item{
  width: 42.5%;
  height: 420rpx;
  background-color: white;
  margin-top: 40rpx;
  margin-left: 37.5rpx;
  float: left;
  border-radius: 20rpx;
  box-shadow: #cecece 2rpx 2rpx 8rpx 8rpx;
}
.item-pic{
  width: 100%;
  height: 240rpx;
  border-radius: 20rpx;
  position: relative;
}
.item-pic image{
  width: 100%;
  height: 100%;
  border-radius: 20rpx;
}
.item-sell{
  position: absolute;
  height: 50rpx;
  color:white;
  padding: 0 10rpx;
  text-align: center;
  line-height: 50rpx;
  border-radius:40rpx;
  right:10rpx;
  top:10rpx;
  background-color:rgba(7, 5, 3,0.2) ;
}
.item-title{
  margin-left: 20rpx;
  margin-top:10rpx;
  font-weight: bolder;
}
.item-price{
  margin-left: 20rpx;
  margin-top:10rpx;
  color:#c16d78;
}
.add{
  position: relative;
  width: 140rpx;
  height: 60rpx;
  text-align: center;
  line-height: 60rpx;
  margin-right: 20rpx;
  float: right;
  color:white;
  background-color: #c16d78;
  border-radius: 40rpx;
}
.movecss{
  position: absolute;
  width: 30rpx;
  height: 30rpx;
  border-radius: 50%;
  top:20rpx;
  right:2rpx;
  background-color: #c16d78;
  z-index: 1;
}

.kong{
  width: 100%;
  height: 150rpx;
}
.kong image{
  width: 100%;
  height: 100%;
  opacity: 0;
}


.joincar{
  width: 100%;
  height: 100rpx;
  position: fixed;
  background-color: white;
  z-index: 2;
  bottom: 0;
}
.he{
  width: 80rpx;
  height: 100rpx;
  text-align: center;
  line-height: 100rpx;
  float: left;
  position: relative;
  margin-left: 40rpx;
  font-size: 36rpx;
}
.num{
  width: 120rpx;
  height: 100rpx;
  line-height: 100rpx;
  margin-left: 20rpx;
  color:#c16d78;
  float: left;
  position: relative;
  font-size: 36rpx;
}
#cart{
  width: 240rpx;
  height: 100rpx;
  text-align: center;
  line-height: 100rpx;
  background-color: #c16d78;
  color: white;
  float: right;
  position: relative;
  font-size: 34rpx;
}
.con{
  width: 55rpx;
  height: 55rpx;
  border-radius: 40rpx;
  text-align: center;
  line-height: 55rpx;
  font-size: 34rpx;
  position: absolute;
  background-color: white;
  color: #c16d78;
  z-index: 4;
  top: 18rpx;
  right: 10rpx;
  
}
