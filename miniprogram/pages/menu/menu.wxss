.showc{
  display: block;
}

.box{
  position: fixed;
  top:80rpx;
  left:-160rpx;
  width: 420rpx;
  height: 100%;
  background-color: rgba(red, green, blue,0);
}
.move{
  width: 280rpx;
  height: 100%;
  z-index: 2;
}
.btn{
  width: 120rpx;
  height: 240rpx;
  position: absolute;
  top:400rpx;
  left:160rpx;
  background-color: #c16d78;
  color:white;
  line-height: 240rpx;
  font-size: 28rpx;
  border-bottom-right-radius: 120rpx;
  border-top-right-radius: 120rpx;
  z-index: 3;
}
.allmenulist{
  width: 160rpx;
  height: 100%;
  background-color: #e6e6e6;
  border-right: 4rpx solid #f2f2f2;
  position: absolute;
  left:-4rpx;
  top:0;
  z-index: 3;
}
.aside-item{
  width: 160rpx;
  height: 120rpx;
  text-align: center;
  line-height: 120rpx;
  border-bottom: 2rpx solid #f2f2f2;
}
.right-box{
  height: 100%;
  width: 600rpx;
  background-color: white;
  float: right;
  margin-top:80rpx;
}
.activem{
  background-color: #c16d78;
  font-weight: bolder;
  color:white;
}
